<script>
    //	主筛选动态
    function doMove_searchType_one(obj){
        $(obj).attr("class","nav_current");
        $(obj).siblings("li").attr("class","nav_link");
    }
    function doMove_searchType_one1(theType){
        theType=theType+"";
        var defaultType="A";
        if(defaultType == theType){
        }
        else{
            $("#disease_"+theType).attr("class","nav_link");
            $("#disease_"+defaultType).attr("class","nav_current");
        }
    }
</script>
