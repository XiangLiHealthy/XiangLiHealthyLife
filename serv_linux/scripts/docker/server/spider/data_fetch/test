<script type="text/javascript">
    var lettersLeft = $('.w_760').position().left + $('.pyContants').position().left;
    $('.pyContants').css('left',lettersLeft);
    var lettersTop = $('#list-info').position().top;
    $(window).scroll(function(){
        if($(this).scrollTop() >= lettersTop){
            $('.pyContants').css('position','fixed');
        }else{
            $('.pyContants').css('position','initial');
        }
    })


    $(function () {
        //  左边侧边栏
        var defaultType='A';
        //$("#defaultType").val();
        $("#disease_"+defaultType).attr("class","nav_current");
    });

    //  拼音栏导航
    function goPyInfo(obj){
        var theType=$(obj).attr("theType");
        var theHeitht=$("#pyInfo_"+theType).offset().top;
        $('body,html').animate({scrollTop:theHeitht},400);
    }
    //  加载更多
    function showMore(obj){
        $(obj).hide();
        $(obj).parent().parent().find("li").show();
    }


    $(".typeInfo_Li a").hover(function(){
        $(this).attr("title",$(this).text());
    },function(){
    });
    $(".morejb_li a").hover(function(){
        $(this).attr("title",$(this).text());
    },function(){
    });
</script>
</body>
</html>