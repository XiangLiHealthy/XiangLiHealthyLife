        <script>
            var searchSet = null;
            var historyRecord = JSON.parse(localStorage.getItem("baikemy_historyRecord"));
            if (historyRecord && historyRecord.length) {
                var html = '';
                for (var i = 0; i < historyRecord.length; i++) {
                    html += '<div class="search-history-list text-circle">' + historyRecord[i] + '</div>';
                }
                $('.search-history').prepend(html);
            }

            $('#search-history').on('click', '.search-history-list', function() {
                $('.search_text').val($(this).text());
                $('.search_wrap').submit();
            })


            $('#search-history,.search_wrap').click(function(e) {
                e.stopPropagation();
                e.preventDefault();
                return false;
            });

            $('.search_text').focus(function(e) {
                var text = $(this).val().trim();
                $('#search-history').removeClass('display-none');
                if (text) {
                    $('.search-history').addClass('display-none');
                    $('#search-history').addClass('display-none');
                    searchMatch(text);
                } else if (historyRecord && historyRecord.length) {
                    $('.search-history').removeClass('display-none');
                    $('.search-result').addClass('display-none');
                } else {
                    $('#search-history').addClass('display-none');
                }
            })
            $(window).click(function() {
                $('#search-history').addClass('display-none');
            });

            $('.search_text').keyup(function(e) {
                var text = $(this).val().trim();
                $('#search-history').addClass('display-none');
                $('.search-history').addClass('display-none');
                $('.search-result').addClass('display-none');
                if (text) {
                    searchMatch(text);
                } else {
                    clearTimeout(searchSet);
                    if ($('.search-history .search-history-list').length) {
                        $('#search-history').removeClass('display-none');
                        $('.search-history').removeClass('display-none');
                    } else {
                        $('#search-history').addClass('display-none');
                    }
                }
            })

            $('.search-result').on('click', '.search-history-list', function() {
                $('.search_text').val($(this).text());
                $('.search_wrap').submit();
            })

            $('#clear-history').click(function(e) {
                $('.search-history .search-history-list').remove();
                localStorage.removeItem("baikemy_historyRecord");
                historyRecord = [];
                $('#search-history').addClass('display-none');
            });

            function search_all() {
                var text = $('.search_text').val().trim()
                if (text) {
                    localStorageSelf(text);
                    return true
                }
                return false
            }

            var keyWord = GetQueryString('keyWord');
            if (keyWord) {
                $('.search_text').val(keyWord);
            }

            function searchMatch(keyWord) {
                clearTimeout(searchSet)
                keyWord = keyWord.replace(/{|}|\[|\]/g, "");;
                searchSet = setTimeout(function() {
                    $.ajax({
                        type: "post",
                        url: "/search/disease/sugest",
                        data: {
                            "keyWord": keyWord,
                            "pageSize": 10
                        },
                        dataType: "json",
                        success: function(data) {
                            var html = '';
                            var data = data.data;
                            for (var i = 0; i < data.length; i++) {
                                html += '<div class="search-history-list text-circle">' + data[i] + '</div>'
                            }

                            $('.search-result').html(html);
                            if (html) {
                                $('#search-history').removeClass('display-none');
                                $('.search-result').removeClass('display-none');
                            } else {
                                $('#search-history').addClass('display-none');
                                $('.search-result').addClass('display-none');
                            }
                        },
                        error: function(err) {
                            console.log(err)
                        }
                    });
                }, 400)
            }

            function GetQueryString(name) {
                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
                var r = window.location.search.substr(1).match(reg);
                if (r != null) return decodeURIComponent(r[2]);
                return null;
            }

            //localStorage存取
            function localStorageSelf(value) {
                var historyRecord = JSON.parse(localStorage.getItem("baikemy_historyRecord"));
                historyRecord = historyRecord ? historyRecord : [];
                var searchTextIndex = historyRecord.indexOf(value);
                if (searchTextIndex >= 0) {
                    historyRecord.splice(searchTextIndex, 1);
                };
                if (value.length > 0) {
                    historyRecord.unshift(value);
                };
                historyRecord = historyRecord.slice(0, 10);
                historyRecord = JSON.stringify(historyRecord);
                localStorage.setItem("baikemy_historyRecord", historyRecord);
            }
        </script>
        <input id="statsShence" type="hidden" value="https://scdatatest.baikemy.com:4006/sa?project=production">
    </div>
