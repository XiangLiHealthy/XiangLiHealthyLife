        <script type="text/javascript">
            //	回到顶部
            $("#top_arrow").hide();
            $(function () {
                $(window).scroll(function(){
                    if ($(window).scrollTop()>100){
                        $("#top_arrow").fadeIn(400);
                    }
                    else
                    {
                        $("#top_arrow").fadeOut(400);
                    }
                });

                $("#top_arrow").click(function(){
                    $('body,html').animate({scrollTop:0},400);
                    return false;
                });
            });
        </script>
